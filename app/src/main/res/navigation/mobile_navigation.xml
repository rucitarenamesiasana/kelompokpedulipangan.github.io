<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.pedulipangan.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.pedulipangan.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_stocks"
        android:name="com.example.pedulipangan.ui.stocks.stocksfragments"
        android:label="Stocks"
        tools:layout="@layout/fragment_stocks" />

    <fragment
        android:id="@+id/navigation_statistics"
        android:name="com.example.pedulipangan.ui.statisticsfragment.statistics_fragment"
        android:label="Stocks"
        tools:layout="@layout/fragment_statistics_fragment" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.pedulipangan.ui.profile.profile_fragment"
        android:label="Stocks"
        tools:layout="@layout/fragment_profile_fragment" />
</navigation>